type":"Character","id":199},{"type":"musicvolume","volume":"0.4","id":200},{"type":"Delay","time":"0.6","id":201},{"type":"Blocker","a":"0","r":"0","g":"0","b":"0","fadetime":"0.6","block":"true","id":202},{"type":"Delay","time":"1","id":203},{"type":"CameraShake","xstrength":"2","ystrength":"2","vibrato":"10","randomness":"50","id":204},{"type":"Character","name":"avg_npc_135#4","fadetime":"1","id":205},{"type":"Delay","time":"1","id":206},{"type":"Dialogue","name":"？？？","text":"怎么回事？！","id":207,"figure":"avg_npc_135#4"},{"type":"Character","name":"avg_npc_134","name2":"avg_npc_134","focus":"1","id":208},{"type":"Dialogue","name":"狱警A","text":"报告巴顿队长，囚犯们打起来了！","id":209,"figure":"avg_npc_134"},{"type":"Character","name":"avg_npc_135#4","id":210},{"type":"Dialogue","name":"巴顿","text":"那你们还在那看着，还不让他们消停？","id":211,"figure":"avg_npc_135#4"},{"type":"Character","name":"avg_npc_134","name2":"avg_npc_134","focus":"2","id":212},{"type":"Dialogue","name":"狱警B","text":"啊，队长，到时候了吗？","id":213,"figure":"avg_npc_134"},{"type":"Character","name":"avg_npc_135#4","id":214},{"type":"Dialogue","name":"巴顿","text":"什么到时候不到时候，管理囚犯是我们狱警的职责！","id":215,"figure":"avg_npc_135#4"},{"type":"Character","name":"avg_npc_134","id":216},{"type":"Dialogue","name":"狱警A","text":"好的好的。","id":217,"figure":"avg_npc_134"},{"type":"Dialogue","name":"狱警A","text":"喂，你们，队长发话了，该停下了！","id":218,"figure":"avg_npc_134"},{"type":"stopmusic","fadetime":"1","id":219},{"type":"CameraShake","stop":"true","id":220},{"type":"Character","name":"avg_npc_133","name2":"avg_npc_133","id":221},{"type":"Dialogue","name":"B区囚犯们","text":"*哥伦比亚俚语*","id":222,"figure":"avg_npc_133"},{"type":"Character","name":"avg_npc_132","name2":"avg_npc_132","id":223},{"type":"Dialogue","name":"A区囚犯们","text":"*哥伦比亚方言*","id":224,"figure":"avg_npc_132"},{"type":"Character","name":"avg_npc_135#1","id":225},{"type":"Dialogue","name":"巴顿","text":"唉，我都说过几次了，这里是监狱，不是你们的战场。","id":226,"figure":"avg_npc_135#1"},{"type":"Dialogue","name":"巴顿","text":"你们应该在这里好好劳动，好好表现自己，这样在外面的亲朋好友们才能早日见到你们，是不是？","id":227,"figure":"avg_npc_135#1"},{"type":"Dialogue","name":"巴顿","text":"我说这些也是为了你们好......","id":228,"figure":"avg_npc_135#1"},{"type":"Dialog","id":229},{"type":"Delay","time":"1","id":230},{"type":"playMusic","intro":"$farce_intro","key":"$farce_loop","volume":"0.4","id":231,"musicid":1},{"type":"Character","name":"char_empty","name2":"avg_npc_133","focus":"2","id":232},{"type":"Dialogue","name":"B区囚犯A","text":"啧，又开始了。","id":233,"figure":"avg_npc_133"},{"type":"Dialog","id":234},{"type":"PlaySound","key":"$rungeneral","volume":"1","block":"false","id":235},{"type":"Character","name":"char_214_kafka_1#4","name2":"avg_npc_133","fadetime":"0.6","focus":"1","id":236},{"type":"Delay","time":"1","id":237},{"type":"Dialogue","name":"卡夫卡","text":"哇，你身上不少伤啊，没事吧？","id":238,"figure":"char_214_kafka_1#4"},{"type":"Character","name":"char_214_kafka_1#4","name2":"avg_npc_133","focus":"2","id":239},{"type":"Dialogue","name":"B区囚犯A","text":"没事没事，小伤，比起这个，我可是放倒了好几个A区的杂碎呢，嘿嘿，值了！","id":240,"figure":"avg_npc_133"},{"type":"Character","name":"char_214_kafka_1#4","name2":"avg_npc_133","focus":"1","id":241},{"type":"Dialogue","name":"卡夫卡","text":"每次都是这么收场的吗？","id":242,"figure":"char_214_kafka_1#4"},{"type":"Character","name":"char_214_kafka_1#4","name2":"avg_npc_133","focus":"2","id":243},{"type":"Dialogue","name":"B区囚犯A","text":"你说巴顿？","id":244,"figure":"avg_npc_133"},{"type":"Dialogue","name":"B区囚犯A","text":"差不多吧，他是狱警们的队长，每次这种时候巴顿都会装模作样地出来制止一下，然后说一些漂亮话。","id":245,"figure":"avg_npc_133"},{"type":"Dialogue","name":"B区囚犯A","text":"尤其现在是监狱停靠在城市的时期，典狱长是不在的，他就搞得跟自己是这座监狱的主人一样。","id":246,"figure":"avg_npc_133"},{"type":"Dialogue","name":"B区囚犯A","text":"蠢货一个。","id":247,"figure":"avg_npc_133"},{"type":"Dialogue","name":"B区囚犯A","text":"不过你说是不是每次都这样，那也不是。","id":248,"figure":"avg_npc_133"},{"type":"Dialogue","name":"B区囚犯A","text":"也有好几次是打上头了收不住......倒不如说收不住了才比较正常，今天这样说停就停才比较少见。","id":249,"figure":"avg_npc_133"},{"type":"Dialogue","name":"B区囚犯A","text":"反正收不住的时候，巴顿一般就会搬出他的救兵。","id":250,"figure":"avg_npc_133"},{"type":"Character","name":"char_214_kafka_1#4","name2":"avg_npc_133","focus":"1","id":251},{"type":"Dialogue","name":"卡夫卡","text":"救兵？","id":252,"figure":"char_214_kafka_1#4"},{"type":"Character","name":"char_214_kafka_1#4","name2":"avg_npc_133","focus":"2","id":253},{"type":"Dialogue","name":"B区囚犯A","text":"喏，来了。","id":254,"figure":"avg_npc_133"},{"type":"Dialog","id":255},{"type":"Character","id":256},{"type":"Delay","time":"1","id":257},{"type":"Character","name":"char_264_Mountain_1#5","fadetime":"1","id":258},{"type":"PlaySound","key":"$d_gen_walk_n","volume":"1","id":259},{"type":"Delay","time":"2","id":260},{"type":"Dialogue","name":"？？？","text":"......","id":261,"figure":"char_264_Mountain_1#5"},{"type":"Character","name":"char_214_kafka_1#2","name2":"avg_npc_133","focus":"1","id":262},{"type":"Dialogue","name":"卡夫卡","text":"安东尼？！","id":263,"figure":"char_214_kafka_1#2"},{"type":"Character","name":"char_214_kafka_1#2","name2":"avg_npc_133","focus":"2","id":264},{"type":"Dialogue","name":"B区囚犯A","text":"看来你真的认得他。","id":265,"figure":"avg_npc_133"},{"type":"Character","name":"char_214_kafka_1#4","name2":"avg_npc_133","focus":"1","id":266},{"type":"Dialogue","name":"卡夫卡","text":"虽然我只看过他穿正服的样子......","id":267,"figure":"char_214_kafka_1#4"},{"type":"Dialog","id":268},{"type":"Character","name":"char_264_Mountain_1#5","id":269},{"type":"Delay","time":"1","id":270},{"type":"Dialog","id":271},{"type":"Blocker","a":"1","r":"0","g":"0","b":"0","fadetime":"0.5","block":"true","id":272},{"type":"CameraEffect","effect":"Grayscale","amount":"0.8","keep":"true","id":273},{"type":"Background","image":"bg_black","screenadapt":"coverall","id":274},{"type":"Character","name":"char_264_Mountain_1#7","id":275},{"type":"Delay","time":"0.5","id":276},{"type":"Blocker","a":"0","r":"0","g":"0","b":"0","fadetime":"0.5","block":"true","id":277},{"type":"Delay","time":"1","id":278},{"type":"Dialog","id":279},{"type":"Blocker","a":"1","r":"0","g":"0","b":"0","fadetime":"0.5","block":"true","id":280},{"type":"CameraEffect","effect":"Grayscale","fadetime":"0","amount":"0","block":"true","id":281},{"type":"Background","image":"bg_undergroundF","screenadapt":"coverall","id":282},{"type":"Character","name":"char_264_Mountain_1#5","id":283},{"type":"Delay","time":"0.5","id":284},{"type":"Blocker","a":"0","r":"0","g":"0","b":"0","fadetime":"0.5","block":"true","id":285},{"type":"Delay","time":"2","id":286},{"type":"Character","name":"avg_npc_135#3","id":287},{"type":"Dialogue","name":"巴顿","text":"你们应该向安东尼先生学习。","id":288,"figure":"avg_npc_135#3"},{"type":"Dialogue","name":"巴顿","text":"虽然身为囚犯，但却崇尚理性，不轻易使用暴力。平时喜欢看书，写字，听音乐，多么高雅！","id":289,"figure":"avg_npc_135#3"},{"type":"Character","name":"char_214_kafka_1#2","name2":"avg_npc_133","focus":"2","id":290},{"type":"Dialogue","name":"B区囚犯A","text":"呕。","id":291,"figure":"avg_npc_133"},{"type":"Character","name":"char_214_kafka_1#4","name2":"avg_npc_133","focus":"1","id":292},{"type":"Dialogue","name":"卡夫卡","text":"你不喜欢安东尼吗？","id":293,"figure":"char_214_kafka_1#4"},{"type":"Character","name":"char_214_kafka_1#4","name2":"avg_npc_133","focus":"2","id":294},{"type":"Dialogue","name":"B区囚犯A","text":"不不不，别搞错了，卡夫卡。","id":295,"figure":"avg_npc_133"},{"type":"Dialogue","name":"B区囚犯A","text":"这座监狱里，没有人不服安东尼的。","id":296,"figure":"avg_npc_133"},{"type":"Dialogue","name":"B区囚犯A","text":"他平时就像巴顿说的那样，对别人很好，但我们都知道，他才是这座监狱里最厉害的家伙。","id":297,"figure":"avg_npc_133"},{"type":"Dialogue","name":"B区囚犯A","text":"据说他刚进监狱的时候可是直接把整个C区的人都给打服了。","id":298,"figure":"avg_npc_133"},{"type":"Dialogue","name":"B区囚犯A","text":"没错，而且虽然他人在C区，一般和我们很少接触，但是他对人真的很好，不管是A区还是B区的，对他都是服气的。","id":299,"figure":"avg_npc_133"},{"type":"Dialogue","name":"B区囚犯A","text":"对对，我呕的是巴顿这家伙的嘴脸。","id":300,"figure":"avg_npc_133"},{"type":"Dialogue","name":"B区囚犯A","text":"你听听，他那是真的在夸安东尼吗？他那是羞辱。","id":301,"figure":"avg_npc_133"},{"type":"Dialogue","name":"B区囚犯A","text":"安东尼是唯一让他抬不起头的对象，但他又必须依靠安东尼来搞定我们，于是他就只能搞这种手段。","id":302,"figure":"avg_npc_133"},{"type":"Character","name":"avg_npc_135#3","id":303},{"type":"Dialogue","name":"巴顿","text":"我们州立曼斯菲尔德监狱，可是首都特批的试点监狱，将来是要作为榜样推广给其他州的。","id":304,"figure":"avg_npc_135#3"},{"type":"Character","name":"avg_npc_133","id":305},{"type":"Dialogue","name":"B区囚犯B","text":"啧，大家都是囚犯当什么榜样啊......","id":306,"figure":"avg_npc_133"},{"type":"Character","name":"avg_npc_135#1","id":307},{"type":"Dialogue","name":"巴顿","text":"总而言之，希望你们能够为自己想一想，也为我想一想！","id":308,"figure":"avg_npc_135#1"},{"type":"Dialogue","name":"巴顿","text":"走吧，安东尼，今天你有一个小时的读书时间。","id":309,"figure":"avg_npc_135#1"},{"type":"Character","name":"char_264_Mountain_1#5","id":310},{"type":"Dialogue","name":"安东尼","text":"嗯。","id":311,"figure":"char_264_Mountain_1#5"},{"type":"Character","name":"char_214_kafka_1#4","name2":"avg_npc_133","focus":"1","id":312},{"type":"Dialogue","name":"卡夫卡","text":"平时一般都见不到安东尼吗？","id":313,"figure":"char_214_kafka_1#4"},{"type":"Character","name":"char_214_kafka_1#4","name2":"avg_npc_133","focus":"2","id":314},{"type":"Dialogue","name":"B区囚犯A","text":"应该是吧？","id":315,"figure":"avg_npc_133"},{"type":"Dialogue","name":"B区囚犯A","text":"他们重犯的时间安排和我们不一样，而且他们大部分时候都呆在C区的塔里。","id":316,"figure":"avg_npc_133"},{"type":"Dialogue","name":"B区囚犯A","text":"怎么，你想见他？","id":317,"figure":"avg_npc_133"},{"type":"Character","name":"char_214_kafka_1#4","name2":"avg_npc_133","focus":"1","id":318},{"type":"Dialogue","name":"卡夫卡","text":"算是吧。","id":319,"figure":"char_214_kafka_1#4"},{"type":"Dialogue","name":"卡夫卡","text":"我想跟他商量一件事，当然，肯定是不能被狱警知道的那种。","id":320,"figure":"char_214_kafka_1#4"},{"type":"Character","name":"char_214_kafka_1#4","name2":"avg_npc_133","focus":"2","id":321},{"type":"Dialogue","name":"B区囚犯A","text":"放心，我懂的。你想和他说些悄悄话的话，倒也不是没有机会......","id":322,"figure":"avg_npc_133"},{"type":"Dialog","id":323},{"type":"Blocker","a":"1","r":"0","g":"0","b":"0","fadetime":"1","block":"true","id":324},{"type":"Character","id":325},{"type":"Image","id":326},{"type":"stopmusic","id":327}],"characters":["B区囚犯A","A区囚犯","卡夫卡","狱警A","狱警B","B区囚犯","？？？","巴顿","B区囚犯们","A区囚犯们","B区囚犯B","安东尼"]}