randomness":"90","fadeout":"true","block":"false","id":47},{"type":"PlaySound","key":"$d_sp_ballista","id":48},{"type":"Blocker","a":"0","r":"255","g":"255","b":"255","fadetime":"0","block":"true","id":49},{"type":"Blocker","a":"1","r":"255","g":"255","b":"255","fadetime":"0.05","block":"true","id":50},{"type":"Blocker","a":"0","r":"255","g":"255","b":"255","fadetime":"0.05","block":"true","id":51},{"type":"Blocker","a":"1","r":"255","g":"255","b":"255","fadetime":"0.05","block":"true","id":52},{"type":"Blocker","a":"0","r":"255","g":"255","b":"255","fadetime":"2","block":"true","id":53},{"type":"Dialog","id":54},{"type":"Delay","time":"1","id":55},{"type":"Character","name":"avg_npc_393_1#1$1","id":56},{"type":"Dialogue","name":"赫德雷","text":"你引爆炸弹的速度变慢了，W。","id":57,"figure":"avg_npc_393_1#1$1"},{"type":"Character","name":"avg_npc_412_1#1$1","id":58},{"type":"Dialogue","name":"W","text":"而你这次手没有发抖呢。","id":59,"figure":"avg_npc_412_1#1$1"},{"type":"dialog","id":60},{"type":"Character","name":"avg_npc_393_1#1$1","id":61},{"type":"CameraShake","duration":"0.3","xstrength":"15","ystrength":"15","vibrato":"20","randomness":"70","fadeout":"true","block":"false","id":62},{"type":"PlaySound","key":"$d_avg_bldwhoosh","volume":"1","id":63},{"type":"Blocker","a":"0","r":"255","g":"255","b":"255","fadetime":"0","block":"true","id":64},{"type":"CameraShake","duration":"1","ystrength":"40","vibrato":"30","randomness":"90","fadeout":"true","block":"false","id":65},{"type":"Blocker","a":"1","r":"255","g":"255","b":"255","fadetime":"0.03","block":"true","id":66},{"type":"PlaySound","key":"$d_avg_bldwhoosh","channel":"bld","delay":"0.1","volume":"1","id":67},{"type":"Blocker","a":"0","r":"255","g":"255","b":"255","fadetime":"0.5","block":"true","id":68},{"type":"PlaySound","key":"$d_gen_walk_n","id":69},{"type":"delay","time":"1","id":70},{"type":"Character","name":"avg_npc_393_1#1$1","fadetime":"0.5","id":71},{"type":"delay","time":"0.51","id":72},{"type":"Dialogue","name":"赫德雷","text":"你从整合运动带走的那些雇佣兵呢？难道你真是一个人来的？","id":73,"figure":"avg_npc_393_1#1$1"},{"type":"Character","name":"avg_npc_412_1#1$1","id":74},{"type":"Dialogue","name":"W","text":"带他们来，然后让他们跟你一样，被摄政王收买，反过来给我一堆惊喜吗？","id":75,"figure":"avg_npc_412_1#1$1"},{"type":"Character","name":"avg_npc_393_1#1$1","id":76},{"type":"Dialogue","name":"赫德雷","text":"......你很为他们着想。难怪，我没有看到更多熟悉的面孔。","id":77,"figure":"avg_npc_393_1#1$1"},{"type":"Character","name":"avg_npc_412_1#1$1","id":78},{"type":"Dialogue","name":"W","text":"我就不一样了，在伦蒂尼姆，我可是见到了很多老熟人。","id":79,"figure":"avg_npc_412_1#1$1"},{"type":"Character","name":"avg_npc_393_1#1$1","id":80},{"type":"Dialogue","name":"赫德雷","text":"你还是老样子，真的很爱从我手下挖人。","id":81,"figure":"avg_npc_393_1#1$1"},{"type":"Character","name":"avg_npc_412_1#10$1","id":82},{"type":"Dialogue","name":"W","text":"让我猜一猜，施瓦布果然已经死了吧？是你亲手杀了他吗，赫德雷？那家伙认识你，比认识我还要久，没错吧？","id":83,"figure":"avg_npc_412_1#10$1"},{"type":"Dialogue","name":"W","text":"杀死一个曾经背着你走了一天一夜，躲过了十几个特雷西斯派来的杀手的老朋友，你握剑的手有没有抖？","id":84,"figure":"avg_npc_412_1#10$1"},{"type":"Character","name":"avg_npc_393_1#1$1","id":85},{"type":"Dialogue","name":"赫德雷","text":"你记错了。护卫罗德岛号的时候，他已经离队了。","id":86,"figure":"avg_npc_393_1#1$1"},{"type":"Character","name":"avg_npc_412_1#7$1","id":87},{"type":"Dialogue","name":"W","text":"好吧，关于这些一起打过仗的人，你的记性一向比我好。","id":88,"figure":"avg_npc_412_1#7$1"},{"type":"Dialogue","name":"W","text":"我早就想问了，记住这些死掉的人，会让你更安心一些吗？","id":89,"figure":"avg_npc_412_1#7$1"},{"type":"Character","name":"avg_npc_393_1#7$1","id":90},{"type":"Dialogue","name":"赫德雷","text":"......","id":91,"figure":"avg_npc_393_1#7$1"},{"type":"Character","name":"avg_npc_412_1#4$1","id":92},{"type":"Dialogue","name":"W","text":"我猜并不会，赫德雷，你的表情告诉我，你最近做的噩梦比当年加起来还要多。","id":93,"figure":"avg_npc_412_1#4$1"},{"type":"dialog","id":94},{"type":"character","fadetime":"0.3","id":95},{"type":"PlaySound","key":"$e_skill_skulsrsword","volume":"0.4","id":96},{"type":"CameraShake","duration":"0.5","xstrength":"15","ystrength":"15","vibrato":"20","randomness":"70","fadeout":"true","block":"false","id":97},{"type":"Blocker","a":"0","r":"255","g":"255","b":"255","fadetime":"0","block":"true","id":98},{"type":"Blocker","a":"1","r":"255","g":"255","b":"255","fadetime":"0.05","block":"true","id":99},{"type":"Blocker","a":"0","r":"255","g":"255","b":"255","fadetime":"0.05","block":"true","id":100},{"type":"Blocker","a":"1","r":"255","g":"255","b":"255","fadetime":"0.05","block":"true","id":101},{"type":"Blocker","a":"0","r":"255","g":"255","b":"255","fadetime":"2","block":"true","id":102},{"type":"Dialog","id":103},{"type":"Delay","time":"0.51","id":104},{"type":"Character","name":"avg_npc_393_1#1$1","id":105},{"type":"Dialogue","name":"赫德雷","text":"W，我认识你的时间和你认识我的时间一样长。","id":106,"figure":"avg_npc_393_1#1$1"},{"type":"Dialogue","name":"赫德雷","text":"当你试图变换角度挑起我的情绪的时候，你只不过是想掩饰你自己内心的波动。","id":107,"figure":"avg_npc_393_1#1$1"},{"type":"Character","name":"avg_npc_412_1#8$1","id":108},{"type":"Dialogue","name":"W","text":"喔，你又知道了。","id":109,"figure":"avg_npc_412_1#8$1"},{"type":"dialog","id":110},{"type":"Character","name":"avg_npc_393_1#1$1","id":111},{"type":"Dialog","id":112},{"type":"CameraShake","duration":"0.5","xstrength":"15","ystrength":"15","vibrato":"20","randomness":"70","fadeout":"true","block":"false","id":113},{"type":"Blocker","a":"1","r":"255","g":"255","b":"255","fadetime":"0.1","block":"true","id":114},{"type":"PlaySound","key":"$e_imp_sword_h","volume":"0.8","id":115},{"type":"Blocker","a":"0","r":"0","g":"0","b":"0","fadetime":"0.8","block":"true","id":116},{"type":"Dialog","id":117},{"type":"CameraShake","duration":"0.5","xstrength":"15","ystrength":"15","vibrato":"20","randomness":"70","fadeout":"true","block":"false","id":118},{"type":"Blocker","a":"1","r":"255","g":"255","b":"255","fadetime":"0.1","block":"true","id":119},{"type":"PlaySound","key":"$p_imp_sword_n","volume":"1","id":120},{"type":"Blocker","a":"0","r":"0","g":"0","b":"0","fadetime":"1","block":"true","id":121},{"type":"Dialog","id":122},