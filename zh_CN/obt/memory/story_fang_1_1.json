0","b":"0","fadetime":"0","block":"true","id":169},{"type":"blocker","a":"1","r":"0","g":"0","b":"0","fadetime":"1","block":"true","id":170},{"type":"character","name":"char_193_frostl_1","id":171},{"type":"delay","time":"1","id":172},{"type":"blocker","a":"0","r":"0","g":"0","b":"0","fadetime":"1","block":"true","id":173},{"type":"delay","time":"1.3","id":174},{"type":"dialogue","name":"霜叶","text":"找到了。队长，酸糖受伤了。","id":175,"figure":"char_193_frostl_1"},{"type":"character","name":"char_366_acdrop","name2":"avg_123_fang_1","focus":"1","id":176},{"type":"dialogue","name":"酸糖","text":"只是轻伤而已，活动活动就没问题了。放心，芬队长，我还能作战。","id":177,"figure":"char_366_acdrop"},{"type":"character","name":"char_366_acdrop","name2":"avg_123_fang_1#2","focus":"2","id":178},{"type":"dialogue","name":"芬","text":"......","id":179,"figure":["avg_123_fang_1",1]},{"type":"character","name":"char_366_acdrop","name2":"avg_123_fang_1#2","focus":"1","id":180},{"type":"dialogue","name":"酸糖","text":"队长？","id":181,"figure":"char_366_acdrop"},{"type":"character","name":"char_366_acdrop","name2":"avg_123_fang_1","focus":"2","id":182},{"type":"dialogue","name":"芬","text":"可以暂时撤退了。镜头，地图上还有其他风险吗？","id":183,"figure":"avg_123_fang_1#1"},{"type":"character","name":"avg_npc_145","name2":"avg_123_fang_1","focus":"1","id":184},{"type":"dialogue","name":"镜头","text":"十区已经完全安全了，并且撤离路线未发现有危险的迹象。","id":185,"figure":"avg_npc_145"},{"type":"character","name":"char_366_acdrop","name2":"avg_123_fang_1","focus":"1","id":186},{"type":"dialogue","name":"酸糖","text":"等等，我没事的。芬队长，还有刚才那三个人......","id":187,"figure":"char_366_acdrop"},{"type":"character","name":"char_366_acdrop","name2":"avg_123_fang_1","focus":"2","id":188},{"type":"dialogue","name":"芬","text":"四人都已经解决了。我们的任务已经完成，确认十区危险状况排除。","id":189,"figure":"avg_123_fang_1#1"},{"type":"dialogue","name":"芬","text":"这里就暂时撤退吧。","id":190,"figure":"avg_123_fang_1#1"},{"type":"dialog","id":191},{"type":"delay","time":"1","id":192},{"type":"character","name":"avg_123_fang_1#2","id":193},{"type":"delay","time":"0.51","id":194},{"type":"dialogue","name":"芬","text":"......抱歉。","id":195,"figure":["avg_123_fang_1",1]},{"type":"dialog","id":196},{"type":"musicvolume","volume":"0.2","fadetime":"3","id":197},{"type":"dialog","id":198},{"type":"blocker","a":"0","r":"0","g":"0","b":"0","fadetime":"0","block":"true","id":199},{"type":"blocker","a":"1","r":"0","g":"0","b":"0","fadetime":"2","block":"true","id":200},{"type":"stopmusic","fadetime":"1","id":201},{"type":"character","id":202},{"type":"background","image":"https://prts.wiki/images/5/5e/Avg_bg_bg_battlefield.png","screenadapt":"coverall","id":203},{"type":"delay","time":"1","id":204},{"type":"playmusic","intro":"$darkalley_intro","key":"https://static.prts.wiki/music/music/act5d0/m_avg_longmendarkalley_loop.mp3","volume":"0.4","id":205,"musicid":1},{"type":"blocker","a":"0","r":"0","g":"0","b":"0","fadetime":"2","block":"true","id":206},{"type":"delay","time":"2","id":207},{"type":"text","text":"十分钟后","id":208},{"type":"dialog","id":209},{"type":"character","name":"char_366_acdrop","name2":"avg_123_fang_1#5","fadetime":"0.5","id":210},{"type":"delay","time":"0.51","id":211},{"type":"character","name":"char_366_acdrop","name2":"avg_123_fang_1#5","focus":"2","id":212},{"type":"dialogue","name":"芬","text":"酸糖......你的伤......","id":213,"figure":["avg_123_fang_1",4]},{"type":"character","name":"char_366_acdrop","name2":"avg_123_fang_1#5","focus":"1","id":214},{"type":"dialogue","name":"酸糖","text":"我没事......咳，真的没事啦，芬队长！滑板已经帮我挡下几乎所有爆炸伤害了，只是跌倒蹭到了一点皮。","id":215,"figure":"char_366_acdrop"},{"type":"character","name":"char_366_acdrop","name2":"avg_123_fang_1","focus":"2","id":216},{"type":"dialogue","name":"芬","text":"对不起......竟然因为我的缘故，让你受了伤。","id":217,"figure":"avg_123_fang_1#1"},{"type":"dialogue","name":"芬","text":"明明应该在十区稳健地完成区域勘察和歼灭，最终安全及时地撤离的。都怪我......","id":218,"figure":"avg_123_fang_1#1"},{"type":"character","name":"char_366_acdrop","name2":"avg_123_fang_1","focus":"1","id":219},{"type":"dialogue","name":"酸糖","text":"队长......？真的没关系啦，你看，真的只是小伤而已，我还留着大部分力气继续参加下一阶段的任务呢！","id":220,"figure":"char_366_acdrop"},{"type":"character","name":"char_193_frostl_1","name2":"avg_123_fang_1","focus":"1","id":221},{"type":"dialogue","name":"霜叶","text":"其他小队应该很快就会来会合。","id":222,"figure":"char_193_frostl_1"},{"type":"dialogue","name":"霜叶","text":"我们的第一阶段任务还是完成了，接下来就等待合流之后进行下一波的正式进攻。","id":223,"figure":"char_193_frostl_1"},{"type":"character","name":"char_193_frostl_1","name2":"avg_123_fang_1#5","focus":"2","id":224},{"type":"dialogue","name":"芬","text":"......我太莽撞了。明明知道那个区域极有可能布下陷阱，还是自己先冲了进去。","id":225,"figure":["avg_123_fang_1",4]},{"type":"character","name":"char_193_frostl_1","name2":"avg_123_fang_1#5","focus":"1","id":226},{"type":"dialogue","name":"霜叶","text":"队长你的判断并没有错。只是在刚才的任务里有些脱节，但是并没有错过一举剿灭的机会。","id":227,"figure":"char_193_frostl_1"},{"type":"dialogue","name":"霜叶","text":"当时只要配合一起进攻，虽然事后看来有些激进，我们的实力是足以击败对方的。","id":228,"figure":"char_193_frostl_1"},{"type":"character","name":"char_366_acdrop","name2":"avg_123_fang_1#5","focus":"1","id":229},{"type":"dialogue","name":"酸糖","text":"其实我也不是不明白芬队长的想法啦。","id":230,"figure":"char_366_acdrop"},{"type":"dialogue","name":"酸糖","text":"刚才站在高处的时候，我能够看到局面。","id":231,"figure":"char_366_acdrop"},{"type":"dialogue","name":"酸糖","text":"队长其实已经找到了对方的弱点，想直接速攻敌人吧？配以后方支援再注意解决陷阱，其实不会产生什么风险。","id":232,"figure":"char_366_acdrop"},{"type":"character","name":"char_366_acdrop","name2":"avg_123_fang_1#5","focus":"2","id":233},{"type":"dialogue","name":"芬","text":"是我指挥的时候判断失误了。我下意识地......将队员的作战方式想成了自己更加熟悉的模拟状况。","id":234,"figure":["avg_123_fang_1",4]},{"type":"dialogue","name":"芬","text":"明明在任务开始前我已经记住了所有人的特长，在正式作战里却还是用了习惯的思路，想当然地发出了不该由酸糖你承担的任务。","id":235,"figure":["avg_123_fang_1",4]},{"type":"dialogue","name":"芬","text":"不仅如此，我并没有清楚地将计划传达给队员......害得大家要消耗更多的精力来完成任务。","id":236,"figure":["avg_123_fang_1",4]},{"type":"character","name":"char_366_acdrop","name2":"avg_123_fang_1#5","focus":"1","id":237},{"type":"dialogue","name":"酸糖","text":"我并不觉得刚才是芬队长有错。刚才的状况下，芬队长让我负责处理陷阱，自己和霜叶去追击敌人是很好的战术。","id":238,"figure":"char_366_acdrop"},{"type":"dialogue","name":"酸糖","text":"只是没有料到敌人在那个情况下还会想要反扑。实在是太不自量力啦，还不是被芬队长你解决掉了。","id":239,"figure":"char_366_acdrop"},{"type":"character","name":"char_336_Scene_1","id":240},{"type":"dialogue","name":"稀音","text":"......","id":241,"figure":"char_336_Scene_1"},{"type":"character","name":"avg_npc_145","name2":"avg_123_fang_1","focus":"1","id":242},{"type":"dialogue","name":"镜头","text":"稀音小姐想说，其实芬队长也很信任自己身边的队员，所以认为大家通过行动能够领会自己的指令吧？","id":243,"figure":"avg_npc_145"},{"type":"character","name":"avg_npc_145","name2":"avg_123_fang_1","focus":"2","id":244},{"type":"dialogue","name":"芬","text":"这......","id":245,"figure":"avg_123_fang_1#1"},{"type":"dialog","id":246},{"type":"character","id":247},{"type":"delay","time":"0.51","id":248},{"type":"character","name":"char_193_frostl_1","name2":"char_366_acdrop","focus":"1","id":249},{"type":"dialogue","name":"霜叶","text":"每个队长的行事风格有所不同。芬队长有自己的擅长之处。","id":250,"figure":"char_193_frostl_1"},{"type":"character","name":"char_193_frostl_1","name2":"char_366_acdrop","focus":"2","id":251},{"type":"dialogue","name":"酸糖","text":"不过我也有些好奇。芬队长说刚才的失误是下意识用了习惯的模拟情况，具体是指什么？","id":252,"figure":"char_366_acdrop"},{"type":"character","name":"avg_123_fang_1#5","id":253},{"type":"dialogue","name":"芬","text":"大概......是我在A1预备小队里的习惯。明明现在不是在小队的模拟演习中，而是更重要的实战现场......","id":254,"figure":["avg_123_fang_1",4]},{"type":"character","name":"avg_123_fang_1#6","id":255},{"type":"camerashake","duration":"0.5","xstrength":"15","ystrength":"15","vibrato":"20","randomness":"70","fadeout":"true","block":"false","id":256},{"type":"dialogue","name":"芬","text":"啊啊，我到底在做什么啊！","id":257,"figure":["avg_123_fang_1",5]},{"type":"character","name":"char_336_Scene_1","id":258},{"type":"dialogue","name":"稀音","text":"不......","id":259,"figure":"char_336_Scene_1"},{"type":"character","name":"avg_npc_145","name2":"avg_123_fang_1","focus":"1","id":260},{"type":"dialogue","name":"镜头","text":"冷静冷静，芬队长。稀音小姐很担心你太过自责。","id":261,"figure":"avg_npc_145"},{"type":"character","name":"char_366_acdrop","name2":"avg_123_fang_1","focus":"1","id":262},{"type":"dialogue","name":"酸糖","text":"放松啦，队长。任务一还算顺利地完成了，调整下情绪轻松起来吧？或者，有苦恼的话也可以说出来，我们现在是一个团队嘛。","id":263,"figure":"char_366_acdrop"},{"type":"dialogue","name":"酸糖","text":"说起A1预备小队......芬所在的预备小队很热闹吧？一起训练、一起进行模拟作战，团队协作能力肯定没得说咯。","id":264,"figure":"char_366_acdrop"},{"type":"character","name":"char_366_acdrop","name2":"avg_123_fang_1","focus":"2","id":265},{"type":"dialogue","name":"芬","text":"谢谢你们，酸糖，稀音，霜叶......还有镜头。","id":266,"figure":"avg_123_fang_1#1"},{"type":"dialogue","name":"芬","text":"实际上......我认为在团队行动能力上，我所在的A1预备小队一定不比别的预备小队差。","id":267,"figure":"avg_123_fang_1#1"},{"type":"dialogue","name":"芬","text":"我们相处了很长时间，大家已经能够完全理解互相的思路和行动方式，有些反应仅凭默契就能做到。","id":268,"figure":"avg_123_fang_1#1"},{"type":"character","name":"char_366_acdrop","name2":"avg_123_fang_1","focus":"1","id":269},{"type":"dialogue","name":"酸糖","text":"真好啊。我看芬经常和米格鲁、克洛丝待在一起，平时也是最好的朋友？","id":270,"figure":"char_366_acdrop"},{"type":"character","name":"char_366_acdrop","name2":"avg_123_fang_1","focus":"2","id":271},{"type":"dialogue","name":"芬","text":"是的......我、米格鲁和克洛丝在来到罗德岛之前，曾在哥伦比亚的警备队做过实习队员。","id":272,"figure":"avg_123_fang_1#1"},{"type":"character","name":"char_366_acdrop","name2":"avg_123_fang_1","focus":"1","id":273},{"type":"dialogue","name":"酸糖","text":"真厉害啊。我在哥伦比亚的时候满脑子只有滑板，来到罗德岛后才发现，之前老警官教给我的弩的技术竟然派上了用场。","id":274,"figure":"char_366_acdrop"},{"type":"dialogue","name":"酸糖","text":"相比而言，我要比芬业余太多了。","id":275,"figure":"char_366_acdrop"},{"type":"character","name":"char_366_acdrop","name2":"avg_123_fang_1","focus":"2","id":276},{"type":"dialogue","name":"芬","text":"可是，酸糖小姐现在已经十分适应战场作战的任务了。","id":277,"figure":"avg_123_fang_1#1"},{"type":"character","name":"char_366_acdrop","name2":"avg_123_fang_1","focus":"1","id":278},{"type":"dialogue","name":"酸糖","text":"嘿嘿，是吗。不过战场受伤我也不是第一次了，总是容易大意地受伤，时常让队长们感到紧张。","id":279,"figure":"char_366_acdrop"},{"type":"character","name":"char_366_acdrop","name2":"avg_123_fang_1","focus":"2","id":280},{"type":"dialogue","name":"芬","text":"再多积累些经验就好了。","id":281,"figure":"avg_123_fang_1#1"},{"type":"dialogue","name":"芬","text":"在A1预备小队的作战里，我和小队成员们经历了很多的训练。","id":282,"figure":"avg_123_fang_1#1"},{"type":"dialogue","name":"芬","text":"在磨合的过程中，甚至摸索出了一套只有我们自己能够理解的方便暗号。","id":283,"figure":"avg_123_fang_1#1"},{"type":"dialogue","name":"芬","text":"有了她们的帮助，给了我能够判断形势的时间和空间，所以才有了成为队长进行指挥的机会。可是......","id":284,"figure":"avg_123_fang_1#1"},{"type":"character","name":"char_366_acdrop","name2":"avg_123_fang_1","focus":"1","id":285},{"type":"dialogue","name":"酸糖","text":"可是？","id":286,"figure":"char_366_acdrop"},{"type":"character","name":"char_366_acdrop","name2":"avg_123_fang_1","focus":"2","id":287},{"type":"dialogue","name":"芬","text":"实际上，今天是我第一次正式作为临时小队队长进行指挥作战......","id":288,"figure":"avg_123_fang_1#1"},{"type":"dialogue","name":"芬","text":"不知道为什么，我无法将我的计划和指挥调整到最好状态......不仅让大家没有能够理解我的指令，还安排给队员不擅长的职责。","id":289,"figure":"avg_123_fang_1#1"},{"type":"character","name":"char_366_acdrop","name2":"avg_123_fang_1#5","focus":"2","id":290},{"type":"dialogue","name":"芬","text":"我......实在想不明白，除了自己能力不足外，到底还有哪里出了问题。","id":291,"figure":["avg_123_fang_1",4]},{"type":"character","name":"char_336_Scene_1","id":292},{"type":"dialogue","name":"稀音","text":"不会......","id":293,"figure":"char_336_Scene_1"},{"type":"character","name":"avg_npc_145","name2":"avg_123_fang_1","focus":"1","id":294},{"type":"dialogue","name":"镜头","text":"怎么可以这么想呢，芬队长！你刚才的反应和判断完全没错啊。","id":295,"figure":"avg_npc_145"},{"type":"dialogue","name":"镜头","text":"我和稀音看得很清楚，在刚才的战场上，你对局势的判断已经十分准确。","id":296,"figure":"avg_npc_145"},{"type":"dialogue","name":"镜头","text":"能够迅速在前线找到敌人的要害，非常勇敢地冲到前方为大家开路。","id":297,"figure":"avg_npc_145"},{"type":"character","name":"avg_npc_145","name2":"avg_123_fang_1","focus":"2","id":298},{"type":"dialogue","name":"芬","text":"只是勇敢的话，在这个战场上无论谁都......","id":299,"figure":"avg_123_fang_1#1"},{"type":"character","name":"char_193_frostl_1","name2":"avg_123_fang_1","focus":"1","id":300},{"type":"dialogue","name":"霜叶","text":"不只是勇敢而已。","id":301,"figure":"char_193_frostl_1"},{"type":"character","name":"char_193_frostl_1","name2":"avg_123_fang_1#4","focus":"2","id":302},{"type":"dialogue","name":"芬","text":"霜叶......？","id":303,"figure":["avg_123_fang_1",3]},{"type":"character","name":"char_193_frostl_1","name2":"avg_123_fang_1#4","focus":"1","id":304},{"type":"dialogue","name":"霜叶","text":"有些人生来就是战士，听从命令的战士。","id":305,"figure":"char_193_frostl_1"},{"type":"dialogue","name":"霜叶","text":"会根据战场情况进行思考判断招式的变化，但是对于长官的命令会绝对服从。","id":306,"figure":"char_193_frostl_1"},{"type":"dialogue","name":"霜叶","text":"但是芬，你不仅仅是战士，也是指挥大家的心脏。","id":307,"figure":"char_193_frostl_1"},{"type":"character","name":"char_193_frostl_1","name2":"avg_123_fang_1#5","focus":"2","id":308},{"type":"dialogue","name":"芬","text":"......我真的能做好吗？","id":309,"figure":["avg_123_fang_1",4]},{"type":"character","name":"char_193_frostl_1","name2":"avg_123_fang_1#5","focus":"1","id":310},{"type":"dialogue","name":"霜叶","text":"嗯。至少，你尽自己所能，让我们的队伍完成了第一次任务。","id":311,"figure":"char_193_frostl_1"},{"type":"character","name":"char_366_acdrop","name2":"avg_123_fang_1","focus":"1","id":312},{"type":"dialogue","name":"酸糖","text":"还有啊，芬队长你既能团结A1预备小队的大家在战场上获得胜利，又能在生活中和大家相处融洽，是个很称职的队长。","id":313,"figure":"char_366_acdrop"},{"type":"dialogue","name":"酸糖","text":"再说了，也不是每个人都能幸运地在预备小队里互相扶持着成长。像我，因为太特立独行了，想加入固定的小队还不给这个机会呢！","id":314,"figure":"char_366_acdrop"},{"type":"dialogue","name":"酸糖","text":"打起精神来嘛，芬队长。","id":315,"figure":"char_366_acdrop"},{"type":"dialogue","name":"酸糖","text":"我们并不认为你下达了错误的命令，你只是想要做得更好。","id":316,"figure":"char_366_acdrop"},{"type":"character","name":"avg_npc_145","name2":"avg_123_fang_1","focus":"1","id":317},{"type":"dialogue","name":"镜头","text":"如果想要寻求改变，就从更多的实战中总结失败经验。芬队长一定能很快解决自己的问题的。","id":318,"figure":"avg_npc_145"},{"type":"character","name":"avg_npc_145","name2":"avg_123_fang_1#2","focus":"2","id":319},{"type":"dialogue","name":"芬","text":"呼......哈......","id":320,"figure":["avg_123_fang_1",1]},{"type":"dialog","id":321},{"type":"delay","time":"2","id":322},{"type":"dialogue","name":"芬","text":"我明白了。","id":323,"figure":["avg_123_fang_1",1]},{"type":"dialogue","name":"芬","text":"我会努力继续履行好队长的职责的。但是我还是希望趁这段时间，大家能够对我的指挥提出一些自己的建议。","id":324,"figure":["avg_123_fang_1",1]},{"type":"character","name":"char_366_acdrop","name2":"avg_123_fang_1","focus":"1","id":325},{"type":"dialogue","name":"酸糖","text":"嗯......大概，刚才我一下子没明白芬队长为什么要冲上去，所以有些慌张？","id":326,"figure":"char_366_acdrop"},{"type":"character","name":"avg_npc_145","name2":"avg_123_fang_1","focus":"1","id":327},{"type":"dialogue","name":"镜头","text":"酸糖小姐，这个归类为“加强沟通”应该更加合适。","id":328,"figure":"avg_npc_145"},{"type":"character","name":"char_366_acdrop","name2":"avg_123_fang_1","focus":"1","id":329},{"type":"dialogue","name":"酸糖","text":"啊，就是这个！","id":330,"figure":"char_366_acdrop"},{"type":"character","name":"char_366_acdrop","name2":"avg_123_fang_1","focus":"2","id":331},{"type":"dialogue","name":"芬","text":"加强沟通......务必让小队成员得到更加明确和清楚的指令......","id":332,"figure":"avg_123_fang_1#1"},{"type":"character","name":"char_193_frostl_1","name2":"avg_123_fang_1","focus":"1","id":333},{"type":"dialogue","name":"霜叶","text":"嗯。沟通很重要。芬的反应和行动很果断，但是一定要确保大家能够及时进行支援。","id":334,"figure":"char_193_frostl_1"},{"type":"character","name":"char_193_frostl_1","name2":"avg_123_fang_1","focus":"2","id":335},{"type":"dialogue","name":"芬","text":"提前制定计划，确保每个队员的安全......","id":336,"figure":"avg_123_fang_1#1"},{"type":"character","name":"avg_npc_145","name2":"avg_123_fang_1","focus":"1","id":337},{"type":"dialogue","name":"镜头","text":"指挥作战是最难、最关键的部分，芬小姐能够成为队长，实力是毋庸置疑的。","id":338,"figure":"avg_npc_145"},{"type":"dialogue","name":"镜头","text":"希望芬队长能够更多鼓励队员，队员也会始终信任队长！","id":339,"figure":"avg_npc_145"},{"type":"character","name":"char_366_acdrop","name2":"avg_123_fang_1","focus":"1","id":340},{"type":"dialogue","name":"酸糖","text":"对了对了，平时A1预备小队的大家除了一起训练，都是怎么增进感情的？","id":341,"figure":"char_366_acdrop"},{"type":"character","name":"char_366_acdrop","name2":"avg_123_fang_1","focus":"2","id":342},{"type":"dialogue","name":"芬","text":"唔，大概就是一起讨论战术和总结，平常就打打闹闹，聊聊天、吵吵架......","id":343,"figure":"avg_123_fang_1#1"},{"type":"character","name":"avg_npc_145","name2":"avg_123_fang_1","focus":"1","id":344},{"type":"dialogue","name":"镜头","text":"吵吵架？真是有够激烈的相处方式啊。","id":345,"figure":"avg_npc_145"},{"type":"character","name":"avg_npc_145","name2":"avg_123_fang_1","focus":"2","id":346},{"type":"dialogue","name":"芬","text":"是啊，像这种时候，都是我强制要求她们进行休息的，不然总是聊着天就忘记了休息时间......","id":347,"figure":"avg_123_fang_1#1"},{"type":"dialogue","name":"芬","text":"距离下次任务还有三十分钟，大家也先休息一下吧。我会负责叫醒大家准时集合。","id":348,"figure":"avg_123_fang_1#1"},{"type":"character","name":"char_366_acdrop","name2":"avg_123_fang_1","focus":"1","id":349},{"type":"dialogue","name":"酸糖","text":"咦？可是我还想听芬队长说更多有关预备小队的故事啊。","id":350,"figure":"char_366_acdrop"},{"type":"character","name":"char_366_acdrop","name2":"avg_123_fang_1","focus":"2","id":351},{"type":"dialogue","name":"芬","text":"......不行，伤员更要好好休息，快点躺下，酸糖。","id":352,"figure":"avg_123_fang_1#1"},{"type":"character","name":"char_366_acdrop","name2":"avg_123_fang_1","focus":"1","id":353},{"type":"dialogue","name":"酸糖","text":"哼哼，好啦，不过任务结束之后，芬队长再给我讲更多有趣的故事吧？我很感兴趣嘛。","id":354,"figure":"char_366_acdrop"},{"type":"character","name":"char_366_acdrop","name2":"avg_123_fang_1","focus":"2","id":355},{"type":"dialogue","name":"芬","text":"我知道了......","id":356,"figure":"avg_123_fang_1#1"},{"type":"character","name":"char_336_Scene_1","id":357},{"type":"dialogue","name":"稀音","text":"......","id":358,"figure":"char_336_Scene_1"},{"type":"character","name":"avg_npc_145","name2":"avg_123_fang_1","focus":"1","id":359},{"type":"dialogue","name":"镜头","text":"稀音想问，您的心情已经恢复了吗？希望刚才的交谈对您有益。","id":360,"figure":"avg_npc_145"},{"type":"character","name":"avg_npc_145","name2":"avg_123_fang_1","focus":"2","id":361},{"type":"dialogue","name":"芬","text":"放心吧，稀音，镜头。只需要再稍微给我一些时间独处，我还有思路需要调整。","id":362,"figure":"avg_123_fang_1#1"},{"type":"character","name":"char_193_frostl_1","name2":"avg_123_fang_1","focus":"1","id":363},{"type":"dialogue","name":"霜叶","text":"......嗯。这样就对了。","id":364,"figure":"char_193_frostl_1"},{"type":"dialog","id":365},{"type":"character","id":366},{"type":"blocker","a":"1","r":"0","g":"0","b":"0","fadetime":"1.5","block":"true","id":367},{"type":"stopmusic","fadetime":"1","id":368},{"type":"background","image":"https://prts.wiki/images/7/7f/Avg_bg_bg_trainingcom.png","screenadapt":"coverall","id":369},{"type":"delay","time":"1","id":370},{"type":"playmusic","intro":"$path_intro","key":"https://static.prts.wiki/music/music/a001/m_dia_path_loop.mp3","volume":"0.4","id":371,"musicid":2},{"type":"blocker","a":"0","r":"0","g":"0","b":"0","fadetime":"1.5","block":"true","id":372},{"type":"delay","time":"1","id":373},{"type":"text","text":"十五日前 罗德岛训练场内","id":374},{"type":"dialog","id":375},{"type":"character","name":"avg_npc_012","fadetime":"1.5","id":376},{"type":"delay","time":"1.5","id":377},{"type":"dialogue","name":"格瑞斯","text":"……今天的训练到此结束。","id":378,"figure":"avg_npc_012"},{"type":"character","name":"char_122_beagle_1","id":379},{"type":"dialogue","name":"米格鲁","text":"呼……好！","id":380,"figure":"char_122_beagle_1"},{"type":"character","name":"char_124_kroos_1","id":381},{"type":"dialogue","name":"克洛丝","text":"终于结束了……呼哈……我已经不行啦……","id":382,"figure":"char_124_kroos_1"},{"type":"character","name":"char_120_hibisc_1","id":383},{"type":"dialogue","name":"芙蓉","text":"克洛丝还是从来不放过任何休息的时间啊……还是听我的建议，和我一起吃健康餐好了？","id":384,"figure":"char_120_hibisc_1"},{"type":"dialogue","name":"芙蓉","text":"会变得更加精神，作息也更有规律。","id":385,"figure":"char_120_hibisc_1"},{"type":"character","name":"char_124_kroos_1","id":386},{"type":"dialogue","name":"克洛丝","text":"不要啦——要有了像芙蓉一样的自制力或许会变得更厉害……但是我还是想轻松一点。","id":387,"figure":"char_124_kroos_1"},{"type":"character","name":"char_122_beagle_1","id":388},{"type":"dialogue","name":"米格鲁","text":"嘘、嘘，克洛丝，教官们都还在呢……！","id":389,"figure":"char_122_beagle_1"},{"type":"character","name":"avg_123_fang_1","id":390},{"type":"dialogue","name":"芬","text":"好啦，不要让格瑞斯教官看笑话，大家整队一起出去……","id":391,"figure":"avg_123_fang_1#1"},{"type":"character","name":"avg_npc_012","id":392},{"type":"dialogue","name":"格瑞斯","text":"芬。你留下。","id":393,"figure":"avg_npc_012"},{"type":"character","name":"avg_123_fang_1#4","id":394},{"type":"dialogue","name":"芬","text":"啊……","id":395,"figure":["avg_123_fang_1",3]},{"type":"delay","time":"0.51","id":396},{"type":"camerashake","duration":"0.5","xstrength":"15","ystrength":"15","vibrato":"20","randomness":"70","fadeout":"true","block":"false","id":397},{"type":"dialogue","name":"芬","text":"是！","id":398,"figure":["avg_123_fang_1",3]},{"type":"character","name":"char_120_hibisc_1","id":399},{"type":"dialogue","name":"芙蓉","text":"咿，好惨。做队长的总是被留得更晚。","id":400,"figure":"char_120_hibisc_1"},{"type":"character","name":"char_124_kroos_1","id":401},{"type":"dialogue","name":"克洛丝","text":"不过，芬的性格也不会反感多和教官交谈几句啦。有责任感又认真，像我完全不能接受比别人多训练，可是芬……","id":402,"figure":"char_124_kroos_1"},{"type":"character","name":"char_122_beagle_1","id":403},{"type":"dialogue","name":"米格鲁","text":"（竟然还有精力开心地摇起尾巴，芬还真是精神啊。）","id":404,"figure":"char_122_beagle_1"},{"type":"character","name":"char_124_kroos_1","id":405},{"type":"dialogue","name":"克洛丝","text":"对哦，芬的情绪一直很好理解，训练的时候也不会感到烦躁。哪里像我们，对于持久作战根本受不了……","id":406,"figure":"char_124_kroos_1"},{"type":"character","name":"char_120_hibisc_1","id":407},{"type":"dialogue","name":"芙蓉","text":"明明想偷懒的就只有你。","id":408,"figure":"char_120_hibisc_1"},{"type":"character","name":"avg_123_fang_1","id":409},{"type":"dialogue","name":"芬","text":"（你们啊……！）","id":410,"figure":"avg_123_fang_1#1"},{"type":"character","name":"char_122_beagle_1","id":411},{"type":"dialogue","name":"米格鲁","text":"噗哈……那我们就先走啦，格瑞斯教官，芬队长！","id":412,"figure":"char_122_beagle_1"},{"type":"dialog","id":413},{"type":"playsound","key":"$d_gen_walk_n","id":414},{"type":"character","fadetime":"1.5","id":415},{"type":"delay","time":"1.5","id":416},{"type":"character","name":"avg_123_fang_1","name2":"avg_npc_012","fadetime":"1","id":417},{"type":"delay","time":"1","id":418},{"type":"character","name":"avg_123_fang_1#5","name2":"avg_npc_012","focus":"1","id":419},{"type":"dialogue","name":"芬","text":"呜……抱歉，教官，让您看到我们这副样子。我们是不是太懒散了……","id":420,"figure":["avg_123_fang_1",4]},{"type":"character","name":"avg_123_fang_1#5","name2":"avg_npc_012","focus":"2","id":421},{"type":"dialogue","name":"格瑞斯","text":"放轻松，你做得不差。这次让你留下，不是因为A1预备小队的事，而是关于你的。","id":422,"figure":"avg_npc_012"},{"type":"character","name":"avg_123_fang_1#4","name2":"avg_npc_012","focus":"1","id":423},{"type":"dialogue","name":"芬","text":"我自己的事……？","id":424,"figure":["avg_123_fang_1",3]},{"type":"character","name":"avg_123_fang_1#4","name2":"avg_npc_012","focus":"2","id":425},{"type":"dialogue","name":"格瑞斯","text":"没错。芬，我曾在几次和A1小队成员的单独谈话中接收到一些信息。有关于你作为队长的担忧。","id":426,"figure":"avg_npc_012"},{"type":"character","name":"avg_123_fang_1#5","name2":"avg_npc_012","focus":"1","id":427},{"type":"dialogue","name":"芬","text":"我……不想让大家太担心的。","id":428,"figure":["avg_123_fang_1",4]},{"type":"character","name":"avg_123_fang_1#5","name2":"avg_npc_012","focus":"2","id":429},{"type":"dialogue","name":"格瑞斯","text":"A1预备小队全员的成长大家有目共睹。训练期结束了之后，队员们也将被派遣去其它临时小队进行更多的实战任务。","id":430,"figure":"avg_npc_012"},{"type":"dialogue","name":"格瑞斯","text":"而你，芬。身为队长，你一直做得不差。","id":431,"figure":"avg_npc_012"},{"type":"character","name":"avg_123_fang_1#5","name2":"avg_npc_012","focus":"1","id":432},{"type":"dialogue","name":"芬","text":"但是……我却很少单独参加临时小队的任务。","id":433,"figure":["avg_123_fang_1",4]},{"type":"dialogue","name":"芬","text":"芙蓉、克洛丝和米格鲁都已经顺利地完成过临时小队的任务，而炎熔更是经常参与各种正式行动，独当一面的实力有目共睹。","id":434,"figure":["avg_123_fang_1",4]},{"type":"dialogue","name":"芬","text":"而身为队长的我，虽然履行着队长的职责，却也受着大家的保护。","id":435,"figure":["avg_123_fang_1",4]},{"type":"dialogue","name":"芬","text":"我明明知道这一点，有时还是会自私地想，如果能一直在A1小队中担任队长也足够了……","id":436,"figure":["avg_123_fang_1",4]},{"type":"dialogue","name":"芬","text":"虽然我希望A1的大家能够一直在一起，但大家也有自己必须要去做的事。所以……我明白，大家都是要向前迈进的。","id":437,"figure":["avg_123_fang_1",4]},{"type":"character","name":"avg_123_fang_1#5","name2":"avg_npc_012","focus":"2","id":438},{"type":"dialogue","name":"格瑞斯","text":"嗯，你的想法我也在和A1成员们的交谈中了解过，她们也都知道你的责任心和心情。","id":439,"figure":"avg_npc_012"},{"type":"dialogue","name":"格瑞斯","text":"实际上，让队员们能够为你疏导情绪也是很重要的。","id":440,"figure":"avg_npc_012"},{"type":"dialogue","name":"格瑞斯","text":"身为前辈，我特别想要告诉你的是，罗德岛的训练成效不仅仅是用战斗能力评级的提升就能概括的。","id":441,"figure":"avg_npc_012"},{"type":"character","name":"avg_123_fang_1","name2":"avg_npc_012","focus":"1","id":442},{"type":"dialogue","name":"芬","text":"是的，我明白！","id":443,"figure":"avg_123_fang_1#1"},{"type":"character","name":"avg_123_fang_1","name2":"avg_npc_012","focus":"2","id":444},{"type":"dialogue","name":"格瑞斯","text":"杜宾教官一直希望，罗德岛的干员们不仅能有过硬的实力和坚定的信念，还要有……不因战场的残酷而封闭自己的，一颗跳动的心脏。","id":445,"figure":"avg_npc_012"},{"type":"dialogue","name":"格瑞斯","text":"谁都不能将自己视作战场上的一颗棋子。","id":446,"figure":"avg_npc_012"},{"type":"dialogue","name":"格瑞斯","text":"因此，芬，属于你的机会会越来越多，不仅能让罗德岛信任你的实力，也能够使你自己更加鲜活。","id":447,"figure":"avg_npc_012"},{"type":"character","name":"avg_123_fang_1#4","name2":"avg_npc_012","focus":"1","id":448},{"type":"dialogue","name":"芬","text":"让我……更加鲜活？","id":449,"figure":["avg_123_fang_1",3]},{"type":"character","name":"avg_123_fang_1#4","name2":"avg_npc_012","focus":"2","id":450},{"type":"dialogue","name":"格瑞斯","text":"小队队长可不是那么好当的。A1预备小队的成员认可了你的实力，也成为了你的知己与同伴，你们的默契旁人难以企及。","id":451,"figure":"avg_npc_012"},{"type":"dialogue","name":"格瑞斯","text":"相信你自己，也要相信不论是哪支小队的队员，在共同的目标面前，即使存在不足，也一定有互相补全的办法。","id":452,"figure":"avg_npc_012"},{"type":"dialogue","name":"格瑞斯","text":"一个人不够完美，两人、三人……汇聚起来的力量，或许会有差异和分歧，但却能在中心更加强大。","id":453,"figure":"avg_npc_012"},{"type":"dialogue","name":"格瑞斯","text":"我一直支持你再进一步，芬。去努力让自己成为引领这份中心力量的人。","id":454,"figure":"avg_npc_012"},{"type":"character","name":"avg_123_fang_1#3","name2":"avg_npc_012","focus":"1","id":455},{"type":"dialogue","name":"芬","text":"教官……","id":456,"figure":["avg_123_fang_1",2]},{"type":"character","name":"avg_123_fang_1","name2":"avg_npc_012","focus":"1","id":457},{"type":"dialogue","name":"芬","text":"……是。我，不会放弃的。","id":458,"figure":"avg_123_fang_1#1"},{"type":"character","name":"avg_123_fang_1","name2":"avg_npc_012","focus":"2","id":459},{"type":"dialogue","name":"格瑞斯","text":"队长的责任和压力可不是谁都能承担和承受的。","id":460,"figure":"avg_npc_012"},{"type":"dialogue","name":"格瑞斯","text":"很好很好，相信自己，记住今天跃跃欲试的自己的心情吧，芬。你能够做到。","id":461,"figure":"avg_npc_012"},{"type":"character","name":"avg_123_fang_1","name2":"avg_npc_012","focus":"1","id":462},{"type":"dialogue","name":"芬","text":"嗯……不过，格瑞斯教官……由你来说这些话……总觉得很奇怪。","id":463,"figure":"avg_123_fang_1#1"},{"type":"character","name":"avg_123_fang_1","name2":"avg_npc_012","focus":"2","id":464},{"type":"dialogue","name":"格瑞斯","text":"啊哈哈……是不是和我平时风格不太像？","id":465,"figure":"avg_npc_012"},{"type":"character","name":"avg_123_fang_1#3","name2":"avg_npc_012","focus":"1","id":466},{"type":"dialogue","name":"芬","text":"倒也没有啦……只是，比平时又专业了很多！","id":467,"figure":["avg_123_fang_1",2]},{"type":"character","name":"avg_123_fang_1#3","name2":"avg_npc_012","focus":"2","id":468},{"type":"dialogue","name":"格瑞斯","text":"哎，你是了解我的，也当然能猜到这些看法是谁真正想告诉你的。","id":469,"figure":"avg_npc_012"},{"type":"dialogue","name":"格瑞斯","text":"不过嘛，即使“她”不主动告诉你这些，也还是希望能够让其他人带给你信心和勇气。","id":470,"figure":"avg_npc_012"},{"type":"character","name":"avg_123_fang_1#7","name2":"avg_npc_012","focus":"1","id":471},{"type":"dialogue","name":"芬","text":"嗯，我明白！","id":472,"figure":["avg_123_fang_1",6]},{"type":"character","name":"avg_123_fang_1#7","name2":"avg_npc_012","focus":"2","id":473},{"type":"dialogue","name":"格瑞斯","text":"好了，快去休息吧。你的第一次单独小队队长任务大概不久后就要指派来了。","id":474,"figure":"avg_npc_012"},{"type":"character","name":"avg_123_fang_1#4","name2":"avg_npc_012","focus":"1","id":475},{"type":"dialogue","name":"芬","text":"咦，这么快？","id":476,"figure":["avg_123_fang_1",3]},{"type":"character","name":"avg_123_fang_1#4","name2":"avg_npc_012","focus":"2","id":477},{"type":"dialogue","name":"格瑞斯","text":"不敢接？","id":478,"figure":"avg_npc_012"},{"type":"character","name":"avg_123_fang_1","name2":"avg_npc_012","focus":"1","id":479},{"type":"dialogue","name":"芬","text":"怎么会！我一定好好准备……","id":480,"figure":"avg_123_fang_1#1"},{"type":"character","name":"avg_123_fang_1","name2":"avg_npc_012","focus":"2","id":481},{"type":"dialogue","name":"格瑞斯","text":"第一次谁都会有失误的，可别怪我没提醒你。到时候别在其他队员面前哭鼻子啊。","id":482,"figure":"avg_npc_012"},{"type":"character","name":"avg_123_fang_1#7","name2":"avg_npc_012","focus":"1","id":483},{"type":"dialogue","name":"芬","text":"我才不会呢！谢谢你，格瑞斯教官！","id":484,"figure":["avg_123_fang_1",6]},{"type":"dialogue","name":"芬","text":"（也谢谢你……杜宾教官。）","id":485,"figure":["avg_123_fang_1",6]},{"type":"dialog","id":486},{"type":"blocker","a":"0","r":"0","g":"0","b":"0","fadetime":"0","block":"true","id":487},{"type":"blocker","a":"1","r":"0","g":"0","b":"0","fadetime":"1","block":"true","id":488},{"type":"character","fadetime":"0.5","id":489},{"type":"delay","time":"1","id":490},{"type":"background","image":"https://prts.wiki/images/8/8a/Avg_bg_bg_black.png","screenadapt":"coverall","id":491},{"type":"delay","time":"1","id":492},{"type":"blocker","a":"0","r":"0","g":"0","b":"0","fadetime":"1","block":"true","id":493},{"type":"character","name":"avg_123_fang_1","fadetime":"1","id":494},{"type":"dialogue","name":"芬","text":"嗯……差点就哭鼻子了，好丢脸。哈哈……","id":495,"figure":"avg_123_fang_1#1"},{"type":"dialogue","name":"芬","text":"大家都在鼓励我，我明明也一直相信的，我能做到。","id":496,"figure":"avg_123_fang_1#1"},{"type":"dialogue","name":"芬","text":"杜宾教官，格瑞斯教官，我……好像能够理解了。被队员信任，被谅解是多么沉重，也多么令人欣慰。","id":497,"figure":"avg_123_fang_1#1"},{"type":"dialogue","name":"芬","text":"我是……队长。想要一直成长，承担更多队长的职责，这是我自己决定的事。","id":498,"figure":"avg_123_fang_1#1"},{"type":"dialog","id":499},{"type":"blocker","a":"0","r":"0","g":"0","b":"0","fadetime":"0","block":"true","id":500},{"type":"blocker","a":"1","r":"0","g":"0","b":"0","fadetime":"2","block":"true","id":501},{"type":"stopmusic","fadetime":"1","id":502},{"type":"character","id":503},{"type":"background","image":"https://prts.wiki/images/5/5e/Avg_bg_bg_battlefield.png","screenadapt":"coverall","id":504},{"type":"delay","time":"1","id":505},{"type":"playmusic","intro":"$batmeeting_intro","key":"https://static.prts.wiki/music/music/act9d0d0/m_avg_batmeeting_loop.mp3","volume":"0.4","id":506,"musicid":3},{"type":"blocker","a":"0","r":"0","g":"0","b":"0","fadetime":"2","block":"true","id":507},{"type":"delay","time":"2","id":508},{"type":"text","text":"三十分钟后","id":509},{"type":"dialog","id":510},{"type":"playsound","key":"$d_gen_walk_n","id":511},{"type":"character","name":"avg_123_fang_1","fadetime":"1.5","id":512},{"type":"delay","time":"2","id":513},{"type":"dialogue","name":"芬","text":"呼......好。","id":514,"figure":"avg_123_fang_1#1"},{"type":"character","name":"avg_123_fang_1#6","id":515},{"type":"dialogue","name":"芬","text":"第三小队集合！","id":516,"figure":["avg_123_fang_1",5]},{"type":"character","name":"char_366_acdrop","name2":"char_336_Scene_1","focus":"1","id":517},{"type":"dialogue","name":"酸糖","text":"呼哈......还好有时间短暂休息了一下。","id":518,"figure":"char_366_acdrop"},{"type":"character","name":"char_366_acdrop","name2":"char_336_Scene_1","focus":"2","id":519},{"type":"dialogue","name":"稀音","text":"嗯......","id":520,"figure":"char_336_Scene_1"},{"type":"character","name":"avg_npc_145","name2":"avg_123_fang_1","focus":"1","id":521},{"type":"dialogue","name":"镜头","text":"虽然给予了休息时间，不过刚才芬队长完全没有闭眼呢。镜头拍了好多芬队长认真翻阅作战档案和资料的认真表情。","id":522,"figure":"avg_npc_145"},{"type":"character","name":"avg_npc_145","name2":"avg_123_fang_1","focus":"2","id":523},{"type":"dialogue","name":"芬","text":"这，这没什么特别的......","id":524,"figure":"avg_123_fang_1#1"},{"type":"dialogue","name":"芬","text":"第二阶段进攻行动将在二十分钟后展开，我们会和第二小队进行联合进攻，再次从十区攻入，快速勘察地形并占据有利位置。","id":525,"figure":"avg_123_fang_1#1"},{"type":"character","name":"char_366_acdrop","name2":"avg_123_fang_1","focus":"1","id":526},{"type":"dialogue","name":"酸糖","text":"最先遇敌交战的小队吗，这个活儿可不轻啊。不过正好适合我们来做。","id":527,"figure":"char_366_acdrop"},{"type":"dialogue","name":"酸糖","text":"芬队长，我们下一步的计划是什么？","id":528,"figure":"char_366_acdrop"},{"type":"character","name":"char_366_acdrop","name2":"avg_123_fang_1","focus":"2","id":529},{"type":"dialogue","name":"芬","text":"嗯，根据上一场交战和勘察的情况，我修改了下作战计划书中的内容，现在配发到各位手上。","id":530,"figure":"avg_123_fang_1#1"},{"type":"dialogue","name":"芬","text":"结合之前作战的经验，我认为还是更有进攻性的主动作战更适合我们。","id":531,"figure":"avg_123_fang_1#1"},{"type":"dialogue","name":"芬","text":"镜头和稀音，务必保证自己的安全，信息沟通无碍。前线小队非常需要精确的地图信息和图像资料传输。","id":532,"figure":"avg_123_fang_1#1"},{"type":"dialogue","name":"芬","text":"酸糖，依旧负责高处援助射击。不需要太过分散，否则不容易进行支援。","id":533,"figure":"avg_123_fang_1#1"},{"type":"character","name":"char_366_acdrop","name2":"avg_123_fang_1","focus":"1","id":534},{"type":"dialogue","name":"酸糖","text":"了解！离敌人近一些，我的弩箭伤害更足。","id":535,"figure":"char_366_acdrop"},{"type":"character","name":"char_193_frostl_1","name2":"avg_123_fang_1","focus":"2","id":536},{"type":"dialogue","name":"芬","text":"霜叶，你和我对敌方部队中心发动进攻。进入现场时以隐蔽状态进行探索。","id":537,"figure":"avg_123_fang_1#1"},{"type":"character","name":"char_193_frostl_1","name2":"avg_123_fang_1","focus":"1","id":538},{"type":"dialogue","name":"霜叶","text":"明白。","id":539,"figure":"char_193_frostl_1"},{"type":"character","name":"avg_npc_145","name2":"avg_123_fang_1","focus":"2","id":540},{"type":"dialogue","name":"芬","text":"镜头得到的地形资料需要以最快速度传递给第二小队，对方会跟在我们身后立即赶来支援。","id":541,"figure":"avg_123_fang_1#1"},{"type":"dialogue","name":"芬","text":"等到突破敌方第一阵线，就立刻引导后续火力的跟进。","id":542,"figure":"avg_123_fang_1#1"},{"type":"dialogue","name":"芬","text":"事前战术安排说明到此为止。在进入十区之后，务必听从我的指令。","id":543,"figure":"avg_123_fang_1#1"},{"type":"dialog","id":544},{"type":"stopmusic","fadetime":"2","id":545},{"type":"delay","time":"2","id":546},{"type":"playmusic","intro":"$exciting_intro","key":"https://static.prts.wiki/music/music/act9d0d0/m_avg_exciting02_loop.mp3","volume":"0.4","id":547,"musicid":4},{"type":"character","name":"avg_123_fang_1#7","fadetime":"0.5","id":548},{"type":"dialogue","name":"芬","text":"刚才......很感谢大家对我的开导。第一次勘察突击作战因为我的缘故造成了队员的受伤，接下来的战场不会再有同样的失误了。","id":549,"figure":["avg_123_fang_1",6]},{"type":"dialogue","name":"芬","text":"我不会再在指令和判断上疏忽。所以，还请大家信任我。","id":550,"figure":["avg_123_fang_1",6]},{"type":"dialogue","name":"芬","text":"身为队长应该做什么，我早就非常了解了。","id":551,"figure":["avg_123_fang_1",6]},{"type":"dialogue","name":"芬","text":"现在......全体人员，进行出发前最后一次随身装备检查。","id":552,"figure":["avg_123_fang_1",6]},{"type":"playsound","key":"$transmission","volume":"1","id":553},{"type":"dialogue","name":"芬","text":"......","id":554,"figure":["avg_123_fang_1",6]},{"type":"delay","time":"1","id":555},{"type":"dialog","id":556},{"type":"delay","time":"1","id":557},{"type":"blocker","a":"0.6","r":"0","g":"0","b":"0","fadetime":"0.5","block":"true","id":558},{"type":"delay","time":"1","id":559},{"type":"subtitle","text":"发现敌方单位。准备作战！","x":"500","y":"370","alignment":"middle","size":"24","delay":"0.04","width":"750","id":560},{"type":"subtitle","id":561},{"type":"dialog","id":562},{"type":"blocker","a":"0","r":"0","g":"0","b":"0","fadetime":"0","block":"true","id":563},{"type":"blocker","a":"1","r":"0","g":"0","b":"0","fadetime":"1","block":"true","id":564},{"type":"stopmusic","fadetime":"1","id":565},{"type":"character","id":566},{"type":"image","id":567}],"characters":["镜头","芬","酸糖","霜叶","？？？","稀音","格瑞斯","米格鲁","克洛丝","芙蓉"]}